<div id="fields" class="dontbreakitplz row">
    <div data-field="f_client_name" class="fieldLineHeight col-xs-12">
        <div class="form-group">
            <label for="f_client_name">
                {#__ key="entity.e_api_credentials.f_client_name"/}&nbsp;
                {@inline_help field="f_client_name"}
                <i data-field="f_client_name" class="inline-help fa fa-info-circle" style="color: #1085EE"></i>
                {/inline_help}
            </label>
            <input class="form-control input" placeholder='{#__ key="entity.e_api_credentials.f_client_name" /}' name="f_client_name_print" value="{f_client_name}" type="text" maxlength="255" readonly="">
</div>
</div>
<div data-field="f_client_key" class="fieldLineHeight col-xs-12">
    <div class="form-group">
        <label for="f_client_key">
                {#__ key="entity.e_api_credentials.f_client_key"/}&nbsp;
                {@inline_help field="f_client_key"}
                    <i data-field="f_client_key" class="inline-help fa fa-info-circle" style="color: #1085EE"></i>
                {/inline_help}
            </label>
	<input class="form-control input" placeholder='{#__ key="entity.e_api_credentials.f_client_key" /}' name="f_client_key_print" value="{f_client_key}" type="text" maxlength="255" readonly="">
</div>
</div>
