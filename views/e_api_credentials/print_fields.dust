                {/inline_help}
            </label>
	<input class="form-control input" placeholder='{#__ key="entity.e_api_credentials.f_token" /}' name="f_token_print" value="{f_token}" type="text" maxlength="255" readonly="">
</div>
</div>
<div data-field="f_token_timeout_tmsp" class="fieldLineHeight col-xs-12">
    <div class="form-group">
        <label for="f_token_timeout_tmsp">
                {#__ key="entity.e_api_credentials.f_token_timeout_tmsp"/}&nbsp;
                {@inline_help field="f_token_timeout_tmsp"}
